  "scripts": {
    "build-dev": "node ./node_modules/better-npm-run build-dev",
    "build-stage": "node ./node_modules/better-npm-run build-stage",
    "build-production": "node ./node_modules/better-npm-run build-production",
    "start-mock-api-local": "node ./node_modules/better-npm-run start-mock-api-local",
    "local": "concurrently --kill-others -r \"npm run start-mock-api-local\" \"node ./node_modules/better-npm-run -s local\"",
    "local-ie": "concurrently --kill-others -r \"npm run start-mock-api-local\" \"node ./node_modules/better-npm-run local-ie\"",
    "dist-server": "node ./node_modules/better-npm-run start-dist-server",
    "unit-dev": "karma start ./test/unit/karma.conf.js",
    "unit": "karma start ./test/unit/karma.conf.js --single-run",
    "unit-coverage": "karma start ./test/unit/karma.coverage.conf.js --single-run"
  },
  "betterScripts": {
    "build-dev": {
      "command": "gulp build",
      "env": {
        "ENV": "dev"
      }
    },
    "build-stage": {
      "command": "gulp build",
      "env": {
        "ENV": "stage"
      }
    },
    "build-production": {
      "command": "gulp build",
      "env": {
        "ENV": "production"
      }
    },
    "start-dist-server": {
      "command": "node ./server/dist.server.js"
    },
    "local": {
      "command": "gulp serve:local",
      "env": {
        "ENV": "local"
      }
    },
    "local-ie": {
      "command": "gulp serve:local-ie",
      "env": {
        "ENV": "local"
      }
    },
    "start-mock-api-local": {
      "command": "node ./node_modules/supervisor/lib/cli-wrapper.js -w ./mockData,./mockApi ./server/mockapi.server.js",
      "env": {
        "ENV": "local",
        "NODE_PATH": "./mockApi"
      }
    }
  },
